<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ua.laposhko.hmt.entity.Place" table="place">
		<id column="id" name="id" type="java.lang.Long">
			<generator class="increment" />
		</id>

		<property column="city_id" name="cityId" type="java.lang.Long" />
		<property column="user_id" name="userId" type="java.lang.Long" />
		
		<property column="name" name="name" type="java.lang.String" />
		<property column="description" name="description" type="java.lang.String" />
		<property column="photo" name="photo" type="java.lang.String" />
		
	</class>
</hibernate-mapping>