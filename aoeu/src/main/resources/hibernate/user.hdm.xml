<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ua.laposhko.hmt.entity.User" table="user">
        <id column="id" name="id" type="java.lang.Long">
            <generator class="increment"/>
        </id>

        <property column="sex_id" name="sexId" type="java.lang.Long"/>
        <property column="country_id" name="city" type="java.lang.Long"/>

        <property column="first_name" name="firstName" type="java.lang.String"/>
        <property column="last_name" name="lastName" type="java.lang.String"/>
        <property column="login" name="login" type="java.lang.String"/>
        <property column="photo" name="photo" type="java.lang.String"/>
        <property column="about_me" name="aboutMe" type="java.lang.String"/>
        <property column="password" name="password" type="java.lang.String"/>

    </class>
</hibernate-mapping>